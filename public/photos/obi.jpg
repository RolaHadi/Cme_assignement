    <script type="text/javascript">

    window.initInput = function(inputText, inputId)
    {
       crtInp = document.getElementById(inputId);
       if (crtInp.value == "")
       {
	       crtInp.value = inputText;
	       crtInp.style.color = "gray";
       }
    }

    window.useInput = function(inputId)
    {
    	 crtInp = document.getElementById(inputId);
    	 if (crtInp.value == "" || crtInp.value == "Name" || crtInp.value == "Email" || crtInp.value == "Comment")
       {
		       crtInp.value = "";
		       crtInp.style.color = "#972483";
       }
    }

    window.submitForm = function()
    {
        var allFieldsSubmitted = true;
        var emailIsValid = true;
        var response = document.getElementById('responseDiv');
        var x = document.getElementById('emailInp').value;
        window.status = 
        	false ;
        if (x != "" && x != 'Email')
        {
        	  var atpos = x.indexOf("@");
            var dotpos = x.lastIndexOf(".");
            if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) 
            {
            	 emailIsValid = false;
                
            }    
        }
        else 
        {
            allFieldsSubmitted = false;
        }
        if (document.getElementById('commentInp').value == "" || document.getElementById('commentInp').value == "Comment")
        {
            allFieldsSubmitted = false;
        }
        if (document.getElementById('nameInp').value == "" || document.getElementById('nameInp').value == "Name")
        {
            allFieldsSubmitted = false;
        }
        
        if (allFieldsSubmitted && emailIsValid)
        {
            document.getElementById('posInp').value = document.body.scrollTop;
            if (window.status == "false")
            {
                response.innerHTML = "Sending message...";document.getElementById('messageForm').submit();            }
            else
            {
            	 response.innerHTML = "You have already sent a message!";
            }
        }
        else if (!allFieldsSubmitted)
        {
        	  response.innerHTML ="All fields are mandatory!";
        }
        else if (!emailIsValid)
        {
        	  response.innerHTML ="Not a valid e-mail address";
        }
    }

    function createGalleryView(type)
    {
    	window.currentPic = 0;
    	window.w = window.innerWidth
    	|| document.documentElement.clientWidth
    	|| document.body.clientWidth;
      //
    	window.h = window.innerHeight
    	|| document.documentElement.clientHeight
    	|| document.body.clientHeight;
      //
      var outerDiv = document.createElement('div');
      outerDiv.style.height = window.h + "px";
      outerDiv.style.zIndex = "16";
      outerDiv.className = "galleryOuter";
      outerDiv.id = "galleryOuter";
      //
      var picDiv = document.createElement('div');
      picDiv.className = "galleryDiv";
      
      var picImg = document.createElement('img');
      picImg.className = "galleryImg";
      
      //picDiv.style.height = 533 + "px";
      //picDiv.style.width = w - 500 + "px";
      //picDiv.style.marginLeft = -((w - 500 + 40) / 2) + "px";
      //picDiv.style.marginTop = -((533 + 40) / 2) + "px";
      picDiv.id = "galleryDiv";
      picImg.id = "galleryImg";
      //
      var closeIcon = document.createElement('div');
      closeIcon.className = "closeIcon";
      if (closeIcon.addEventListener)
      {
	      closeIcon.addEventListener('click', function () 
	      {
	         if (!window.canGoNext)
	         {
	        	 window.pictureLoading(true);
	        	 galleryImg.src = "";
	         }
		     	window.currentPic = 0;
		     	document.getElementById('galleryOuter').style.display = "none";
	        	document.body.removeChild(outerDiv);
	      });
      }
      else
      {
	      closeIcon.attachEvent('onclick', function () 
	      {
	         if (!window.canGoNext)
	         {
	        	 window.pictureLoading(true);
	        	 galleryImg.src = "";
	         }
		     	window.currentPic = 0;
		     	document.getElementById('galleryOuter').style.display = "none";
	        	document.body.removeChild(outerDiv);
	      });
      }
      //
      var prevIcon = document.createElement('div');
      prevIcon.className = "prevIcon";
      prevIcon.id = "prevIcon";
      if (prevIcon.addEventListener)
      {
	      prevIcon.addEventListener('click', function () 
	      {
	          movePic(type, false);
	      });
      }
      else
      {
	      prevIcon.attachEvent('onclick', function () 
	      {
	          movePic(type, false);
	      });
      }
      prevIcon.style.display = "none";      
      //
      var nextIcon = document.createElement('div');
      nextIcon.className = "nextIcon";
      nextIcon.id = "nextIcon";
      if (nextIcon.addEventListener)
      {
	      nextIcon.addEventListener('click', function () 
	      {
	          movePic(type, true);
	      });
      }
      else
      {
	      nextIcon.attachEvent('onclick', function () 
	      {
	          movePic(type, true);
	      });
      }
      //
      outerDiv.appendChild(picDiv);
      picDiv.appendChild(picImg);
      picDiv.appendChild(prevIcon); 
      picDiv.appendChild(nextIcon); 
      picDiv.appendChild(closeIcon); 
      document.body.appendChild(outerDiv);
    }

    function movePic(type, next)
    {
        var container = document.getElementById('galleryDiv');
        var arrowLeft = document.getElementById('prevIcon');
        var arrowRight = document.getElementById('nextIcon');
        var nbPicsPhoto = 
        14;
        var nbPicsDish = 
        16;
        var nbPicsSushi = 
        35;
        if (window.canGoToNext)
        {
	        next ? window.currentPic++ : window.currentPic--;
	        if (window.currentPic <= 1)
	        {
	            arrowLeft.style.display = "none";
	        }
	        else
	        {
	            arrowLeft.style.display = "block";
	        }
	
	        if (type == "photo")
	        {
			        if (window.currentPic == nbPicsPhoto)
			        {
			            arrowRight.style.display = "none";
			        }
			        else
			        {
			            arrowRight.style.display = "block";
			        }
	        }
	        else if (type == "dish")
	        {
			        if (window.currentPic == nbPicsDish)
			        {
			            arrowRight.style.display = "none";
			        }
			        else
			        {
			            arrowRight.style.display = "block";
			        }
	        }
	        else
	        {
	        	if (window.currentPic == nbPicsSushi)
		        {
		            arrowRight.style.display = "none";
		        }
		        else
		        {
		            arrowRight.style.display = "block";
		        }
	        }
	        //
        	  window.pictureLoading(false);
	        if (((next && window.currentPic > 0) || (!next && window.currentPic > 1)) && ((type == "photo" && window.currentPic < nbPicsPhoto) || (type == "dish" && window.currentPic < nbPicsDish) || (type == "sushi" && window.currentPic < nbPicsSushi)))
	        {
	            var nextImg = new Image();
	            nextImg.src = "gallery/" + type + "/" + type + "_gallery (" + (next ? window.currentPic + 1 : window.currentPic - 1) + ").jpg";
	        }
	        if (((next && window.currentPic > 1) || (!next && window.currentPic > 2)) && ((type == "photo" && window.currentPic < (nbPicsPhoto - 1)) || (type == "dish" && window.currentPic < (nbPicsDish - 1)) || (type == "sushi" && window.currentPic < (nbPicsSushi - 1))))
	        {
	            var nextSImg = new Image();
	            nextSImg.src = "gallery/" + type + "/" + type + "_gallery (" + (next ? window.currentPic + 2 : window.currentPic - 2) + ").jpg";
	        }
	        var currentImg = document.getElementById('galleryImg');
	        currentImg.style.height = 'auto';
	        currentImg.style.width = 'auto';
	        currentImg.src = "gallery/" + type + "/" + type + "_gallery (" + window.currentPic + ").jpg";
	        if (currentImg.addEventListener)
	        {
		        currentImg.addEventListener('load', function () 
		        {
		        		window.pictureLoading(true);
		        });
	        }
	        else
	        {
		        currentImg.attachEvent('onload', function () 
		        {
		        		window.pictureLoading(true);
		        });
	        }
	        var currentWidth;
	        var currentHeight;
	        if (currentImg.width > currentImg.height)
	        {
	        	  currentWidth = window.w - 500;
	        	  currentHeight = currentWidth * 615 / 922;
	        }
	        else if (currentImg.width < currentImg.height)
	        {
	        	  currentWidth = window.w - 900;
	        	  currentHeight = currentWidth * 922 / 615;
	        }
	        else
	        {
	        	currentWidth = window.w - 800;
	        	currentHeight = currentWidth;
	        }
	        currentImg.style.width = currentWidth + "px";
	        currentImg.style.height = currentHeight + "px";;
	        container.style.width = currentWidth + "px";
	        container.style.height = currentHeight - 2 + "px";
	        currentImg.width = currentWidth;
	        currentImg.height = currentHeight;
	        //container.style.background = "url('gallery/" + type + "/" + type + "_gallery (" + window.currentPic + ").jpg')";
	        //container.style.background += " no-repeat center center fixed";
	        container.style.marginLeft = -(currentWidth / 2) - 20 + "px";
	        container.style.marginTop = -(currentHeight / 2) - 20 + "px";
        }
        else
        {
            
        }
    }

    window.displayGallery = function(type)
    {
    	 createGalleryView(type);
    	 window.canGoToNext = true;
    	 movePic(type, true);
    }

    window.pictureLoading = function (state)
    {
      var arrowLeft = document.getElementById('prevIcon');
      var arrowRight = document.getElementById('nextIcon');
      if (!state)
      {
    	  	window.canGoToNext = false;
	      galleryDiv.appendChild(load_screen);
	      galleryImg.className = "galleryImgLoading";
	      spinner.className = "load_logo";
	      window.arrowRightOldDisplay = arrowRight.style.display;
	      window.arrowLeftOldDisplay = arrowLeft.style.display;
	      arrowRight.style.display = "none";
	      arrowLeft.style.display = "none";
      }
      else
      {
    	  	window.canGoToNext = true;
  			document.getElementById("tempCache").appendChild(load_screen);
	      galleryImg.className = "galleryImg";
	      arrowRight.style.display = window.arrowRightOldDisplay;
	      arrowLeft.style.display = window.arrowLeftOldDisplay;
      }

    }

    </script>
